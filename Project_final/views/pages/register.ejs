<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<body>
    <div class="container">
      <h1 class="mb-4">Register Akun</h1>
        <form action="/register" method="POST" id="contactform" enctype="multipart/form-data">
            <div class="form-group">
                <label for="nama_acc">Username</label>
                <input type="text" name="nama_acc" placeholder="Masukkan Username" class="form-control">
            </div>

            <div class="form-group">
                <label for="nama_cust">Nama</label>
                <input type="text" name="nama_cust" placeholder="Masukkan Nama" class="form-control">
            </div>

            <div class="form-group">
                <label for="email">email</label>
                <input type="text" name="email" placeholder="Masukkan Email anda" class="form-control">
            </div>

            <div class="form-group">
                <label for="nomor_telfon">Nomor Telpon</label>
                <input type="text" name="nomor_telfon" placeholder="Nomor Telpon" class="form-control">
            </div>

            <div class="form-group">
                <label for="alamat_rumah">Alamat</label>
                <input type="text" placeholder="Alamat" name="alamat_rumah" class="form-control"/>
            </div>
            
            <a href="/" class="btn btn-secondary">Cancel</a>
            <button type="submit" class="btn btn-primary">Save</button>
        </form>
    </div>
</body>